{
    "name": "p4pdf/p4pdf-php",
    "description": "P4PDF PHP API - Professional PDF Processing Library",
    "type": "library",
    "homepage": "https://p4pdf.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "P4PDF",
            "email": "info@p4pdf.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "P4Pdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.21"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
